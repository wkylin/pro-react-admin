{
  "version": 2,
  "buildCommand": "npm run build:mf:vercel",
  "outputDirectory": "dist-vercel",
  "routes": [
    { "handle": "filesystem" },

    { "src": "/projectA(/.*)?$", "dest": "/projectA/index.html" },
    { "src": "/projectB(/.*)?$", "dest": "/projectB/index.html" },

    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
