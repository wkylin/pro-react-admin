{
  "name": "pro-react-admin",
  "version": "2.0.0",
  "description": "pro-react-admin",
  "keywords": [
    "React",
    "JSX",
    "React Router",
    "ESLint",
    "TSX",
    "Vite",
    "Bit",
    "Bit.dev",
    "Bit.cloud"
  ],
  "homepage": "https://wkylin.github.io/pro-react-admin",
  "main": "index.js",
  "sideEffects": true,
  "scripts": {
    "http-server": "http-server ./dist -p 9090 --proxy https://my-json-server.typicode.com -o --cors --log-ip --gzip",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:jest": "jest",
    "update:yarn": "yarn upgrade-interactive --latest",
    "update:npm": "ncu -u && npm install",
    "lint:fix": "npm run eslint:fix && npm run stylelint:fix && npm run prettier:fix",
    "lint": "npm run eslint && npm run stylelint && npm run prettier:check",
    "stylelint": "stylelint \"src/**/*.less\" --custom-syntax postcss-less",
    "stylelint:fix": "stylelint \"src/**/*.less\" --fix --custom-syntax postcss-less",
    "prettier:check": "prettier --check \"src/**/*\" --end-of-line auto --ignore-unknown",
    "prettier:fix": "prettier --write \"src/**/*\" --end-of-line auto --ignore-unknown",
    "prettier:fix:all": "prettier --write . --end-of-line auto --ignore-unknown",
    "eslint": "eslint --cache ./src",
    "eslint:fix": "eslint --fix --cache ./src",
    "markdownlint": "markdownlint *.md",
    "markdownlint:fix": "markdownlint --fix *.md",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major",
    "push:tags": "git push --follow-tags origin pre-gh-pages",
    "prepare": "husky && husky install",
    "predeploy": "npm run build:production",
    "deploy": "gh-pages -d dist",
    "vite:dev": "concurrently -r \"vite -- --host\"",
    "vite:dev:faker": "concurrently -r \"npm run vite:dev\"",
    "vite:build": "vite build",
    "vite:preview": "vite preview",
    "vite:start:dev": "vite --mode dev",
    "vite:build:production": "vite build --mode production",
    "clean:lock": "rm -rf package-lock.json yarn.lock pnpm-lock.yaml",
    "clean:dist": "rm -rf ./dist",
    "clean:modules": "rm -rf node_modules"
  },
  "author": "wkylin",
  "contributors": [
    {
      "name": "wkylin",
      "email": "wkylin.w@gmail.com",
      "url": "https://github.com/orgs/aresray/teams"
    }
  ],
  "bugs": {
    "url": "https://github.com/wkylin/pro-react-admin/issues",
    "email": "wkylin.w@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.30.0",
    "@vitejs/plugin-react": "^4.6.0",
    "babel-jest": "^30.0.2",
    "commitizen": "^4.3.1",
    "concurrently": "^9.2.0",
    "conventional-changelog-cli": "^5.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.30.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "http-server": "^14.1.1",
    "husky": "^9.1.7",
    "less": "^4.3.0",
    "lint-staged": "^16.1.2",
    "markdownlint-cli": "^0.45.0",
    "mini-css-extract-plugin": "^2.9.2",
    "mockjs": "^1.1.0",
    "nodemon": "^3.1.10",
    "npm-check-updates": "^18.0.1",
    "pnpm": "^10.12.4",
    "postcss": "^8.5.6",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-less": "^6.0.0",
    "postcss-loader": "^8.1.1",
    "postcss-normalize": "^13.0.1",
    "postcss-preset-env": "^10.2.4",
    "pre-commit": "^1.2.2",
    "prettier": "^3.6.2",
    "pretty-quick": "^4.2.2",
    "react-refresh": "^0.17.0",
    "react-test-renderer": "^19.1.0",
    "rimraf": "^6.0.1",
    "rollup-plugin-visualizer": "^6.0.3",
    "serve": "^14.2.4",
    "standard": "^17.1.2",
    "standard-version": "^9.5.0",
    "style-loader": "^4.0.0",
    "stylelint": "^16.21.0",
    "stylelint-config-standard": "^38.0.0",
    "typescript": "^5.8.3",
    "vite": "^7.0.0",
    "vite-plugin-html": "^3.2.2"
  },
  "dependencies": {
    "@ant-design/cssinjs": "^1.23.0",
    "@ant-design/icons": "^6.0.0",
    "@ant-design/v5-patch-for-react-19": "^1.0.3",
    "antd": "^5.26.2",
    "axios": "^1.10.0",
    "dayjs": "^1.11.13",
    "i18next": "^25.2.1",
    "i18next-browser-languagedetector": "^8.2.0",
    "install": "^0.13.0",
    "nanoid": "^5.1.5",
    "react": "^19.1.0",
    "react-css-theme-switcher": "^0.3.0",
    "react-dom": "^19.1.0",
    "react-error-boundary": "^6.0.0",
    "react-i18next": "^15.5.3",
    "react-router-dom": "^7.6.3",
    "react-sticky": "^6.0.3",
    "react-sticky-box": "^2.0.5",
    "sanitize.css": "^13.0.0",
    "screenfull": "^6.0.2",
    "typescript": "^5.8.3",
    "vite-plugin-imp": "^2.4.0",
    "vite-plugin-svgr": "^4.3.0"
  },
  "engines": {
    "node": ">= 14.17.1",
    "npm": ">= 7.13.0",
    "yarn": ">= 1.22.10"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js, jsx, ts, tsx}": [
      "npm run eslint:fix"
    ],
    "src/**/*.less": [
      "npm run stylelint:fix"
    ],
    "*.md": [
      "npm run markdownlint:fix"
    ],
    "**/*": [
      "npm run prettier:fix"
    ]
  },
  "jest": {
    "collectCoverage": true
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
