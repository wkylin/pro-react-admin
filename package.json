      "post-merge": "install-changed",
      "post-commit": "exec < /dev/tty && node webpack/check-version.js"
    }
  },
  "lint-staged": {
    "src/**/*.{js, jsx, ts, tsx}": [
      "npm run eslint:fix"
    ],
    "src/**/*.less": [
      "npm run stylelint:fix"
    ],
    "*.md": [
      "npm run markdownlint:fix"
    ],
    "**/*": [
      "npm run prettier:fix"
    ]
